<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
     android:background="@drawable/e"
    tools:context=".MainActivity" >

   <Spinner
       android:id="@+id/Spinnerfive"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/ture_ok"
       android:layout_centerHorizontal="true" />
   <TextView
       android:id="@+id/Text_type"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentTop="true"
       android:layout_marginTop="12dp"
       android:text="@string/Text_type"
       android:textSize="20sp" />

   <Button
       android:id="@+id/ture_ok"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/Text_type"
       android:layout_alignRight="@+id/Spinnerfive"
       android:layout_marginRight="14dp"
       android:text="@string/ture_ok"
       android:textSize="12sp" />

 <Spinner
       android:id="@+id/Spinnersix"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/tu_ok"
       android:layout_centerHorizontal="true" />

   <Button
       android:id="@+id/tu_ok"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/Text_analytic"
       android:layout_alignRight="@+id/Spinnersix"
       android:layout_marginRight="14dp"
       android:text="@string/tu_ok"
       android:textSize="12sp" />

   <TextView
       android:id="@+id/Text_analytic"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/Spinnersix"
       android:layout_below="@+id/Spinnerfive"
       android:layout_marginTop="70dp"
       android:text="@string/Text_analytic"
       android:textSize="20sp" />

</RelativeLayout>

